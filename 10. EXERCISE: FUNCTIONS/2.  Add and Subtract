def add_and_subtract(first_number, second_number, third_number, result):
    result = sum(first_number, second_number, result)
    result = subtract(third_number, result)
    return result


def sum(first_number, second_number, result):
    result = first_number + second_number
    return result


def subtract(third_number, result):
    result -= third_number
    return result


first_number = int(input())
second_number = int(input())
third_number = int(input())
result = 0
print(add_and_subtract(first_number, second_number, third_number, result))
