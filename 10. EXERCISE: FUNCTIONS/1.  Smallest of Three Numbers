def calculation(first_number, second_number, third_number, smallest_number):
    smallest_number = first_number
    if second_number < smallest_number:
        smallest_number = second_number
    if third_number < smallest_number:
        smallest_number = third_number
    return smallest_number


first_number = int(input())
second_number = int(input())
third_number = int(input())
smallest_number=0
print(calculation(first_number, second_number, third_number, smallest_number))
