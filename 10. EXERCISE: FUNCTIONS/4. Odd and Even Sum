def caluclating(number_as_string, odd_sum, even_sum):
    for letter in number_as_string:
        letter = int(letter)
        if letter % 2 == 0:
            even_sum += letter
        else:
            odd_sum += letter

    return f"Odd sum = {odd_sum}, Even sum = {even_sum}"


number = int(input())
number_as_string = str(number)
odd_sum = 0
even_sum = 0
print(caluclating(number_as_string, odd_sum, even_sum))
